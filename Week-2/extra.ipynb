    "import csv\n",
    "total_spend = 0\n",
    "with open ('uber_eats.csv') as data_file:\n",
    "    data=csv.reader(data_file)\n",
    "    for row in data:\n",
    "      #find the total amount spend on uber eats\n",
    "      if row[2] != 'price':\n",
    "        total_spend += float(row[2])\n",
    "\n",
    "print(total_spend)"
   ]
  }
